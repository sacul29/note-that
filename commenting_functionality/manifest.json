{
  "name": "LC1",
  "description": "Provides summaries of URLS on mousing over, if a summary can not be found print error. Written by, Lucas Chaufournier",
  "author":"Lucas F. Chaufournier",
  "version": "1.0",
  "permissions": [
    "tabs", "http://*/*", "https://*/*","activeTab"
  ],
 "content_scripts": [
    {
      "matches": ["http://*/*","https://*/*"],
      "css": ["core.css"],
      "js": ["core.js"],
      "run_at": "document_end",
      "all_frames": true
    }
  ],
  "browser_action": {
      "default_title": "My first extension.",
      "default_icon": "icon.png",
      "default_popup": "main.html"
  },
  "manifest_version": 2
}
